{
  "kind": "collectionType",
  "collectionName": "changelogs",
  "info": {
    "singularName": "changelog",
    "pluralName": "changelogs",
    "displayName": "Changelog"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Version": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "default": "1.0.0",
      "unique": true
    },
    "publishDate": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "date",
      "default": "2023-12-16",
      "required": true
    },
    "Main": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "light"
      },
      "customField": "plugin::ckeditor.CKEditor"
    }
  }
}
